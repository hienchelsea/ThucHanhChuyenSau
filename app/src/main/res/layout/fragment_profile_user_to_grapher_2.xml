<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorWhile"
    android:clickable="true"
    android:focusable="true"
    >
    <LinearLayout
        android:id="@+id/llToolbar"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/colorPrimary"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imgBack"
            android:layout_width="@dimen/img_width_width"
            android:layout_height="@dimen/img_width_width"
            android:padding="16dp"
            android:src="@drawable/icon_back" />

        <TextView
            android:id="@+id/tvSearch"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="Profile"
            android:textColor="@color/colorWhile"
            android:textSize="@dimen/dp18"
            android:visibility="visible"


            />

        <TextView
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:gravity="center" />

    </LinearLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:text="Them 1 dich vu moi"
                android:gravity="center"
                android:textSize="@dimen/dp16"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                />
            <LinearLayout
                android:padding="@dimen/dp16"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:background="@color/colorWhileDark"
                    android:layout_marginTop="@dimen/dp16"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView
                        android:gravity="center|left"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:text="Ten dich vu"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"

                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtNameOption"
                        android:inputType="textEmailAddress"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        android:background="#00ffffff"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView
                        android:gravity="center|left"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:text="The loai"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"

                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtType"
                        android:inputType="textEmailAddress"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        />
                </LinearLayout>
                <LinearLayout
                    android:background="@color/colorWhileDark"
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView

                        android:gravity="center|left"
                        android:layout_width="130dp"
                        android:layout_height="match_parent"
                        android:text="So luong anh chup"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"

                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        android:background="#00ffffff"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtCountImg"
                        android:inputType="number"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView
                        android:gravity="center|left"
                        android:layout_width="130dp"
                        android:layout_height="match_parent"
                        android:text="So luong anh in"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"

                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtPrintImg"
                        android:inputType="number"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        />
                </LinearLayout>
                <LinearLayout
                    android:background="@color/colorWhileDark"
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView
                        android:gravity="center|left"
                        android:layout_width="130dp"
                        android:layout_height="match_parent"
                        android:text="Gia theo ngay"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        android:background="#00ffffff"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtTotalDay"
                        android:inputType="number"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="56dp">
                    <TextView
                        android:gravity="center|left"
                        android:layout_width="130dp"
                        android:layout_height="match_parent"
                        android:text="Gia theo gio"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        />
                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/edtTotalHour"
                        android:inputType="number"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        />
                </LinearLayout>


                <LinearLayout
                    android:background="@color/colorWhileDark"
                    android:layout_marginTop="@dimen/dp16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:gravity="center_vertical"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        android:layout_width="match_parent"
                        android:layout_height="56dp"
                        android:text="Ghi chu" />

                    <EditText
                        android:paddingLeft="@dimen/dp16"
                        android:id="@+id/edtIntroduce"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:minLines="3"
                        android:textSize="@dimen/dp14"
                        android:textColor="@color/colorBlack"
                        android:background="#00ffffff"
                        />

                </LinearLayout>
            </LinearLayout>

            <Button
                android:layout_margin="@dimen/dp16"
                android:layout_marginBottom="@dimen/dp16"
                android:id="@+id/btnNextStep"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="@drawable/hinh_chu_nhat_xanh"
                android:text="Buoc 3"
                android:textColor="@color/colorWhile"
                />

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>


</LinearLayout>